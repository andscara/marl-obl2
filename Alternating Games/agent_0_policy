	0 -> "0b" [label=b]
	"0p" [label="0p\n[p: 1.00, b: 0.00]"]
	0 -> "0p" [label=p]
	"0pb" [label="0pb\n[p: 0.50, b: 0.50]"]
	"0p" -> "0pb" [label=b]
	1 [label="1\n[p: 1.00, b: 0.00]"]
	"1b" [label="1b\n[p: 0.50, b: 0.50]"]
	1 -> "1b" [label=b]
	"1p" [label="1p\n[p: 1.00, b: 0.00]"]
	1 -> "1p" [label=p]
	"1pb" [label="1pb\n[p: 1.00, b: 0.00]"]
	"1p" -> "1pb" [label=b]
	2 [label="2\n[p: 1.00, b: 0.00]"]
	"2b" [label="2b\n[p: 0.00, b: 1.00]"]
	2 -> "2b" [label=b]
	"2p" [label="2p\n[p: 1.00, b: 0.00]"]
	2 -> "2p" [label=p]
	"2pb" [label="2pb\n[p: 0.50, b: 0.50]"]
